<div>
  <label>Bucket Name:</label>
  <!-- gs://<bucketname>/<prefix> -->
  <input type="hidden" name="storage-url" value="{{storage_url}}"/>
  <input type="hidden" name="backend-login" value="oauth2"/>

  <input type="text" name="bucketname" placeholder="(Max 32, Alphanumeric)" autocomplete="off" value="{{bucketname}}"/>
  <label>Prefix:</label>
  <input type="text" name="prefix" placeholder="(Max 32, Alphanumeric)" autocomplete="off" value="{{prefix}}"/>
</div>

<div>
  <label>Access Secret: <a href="https://www.google.com/device" id="get_google_key" target="_blank">Get Key</a></label>
  <div id="get_google_key_code"></div>
  <input type="text" name="backend-password" value="{{backend_password}}" id="get_google_key_token"/>
</div>

<div>
  <label>Filesystem Passphrase:</label>
  <input type="text" name="fs-passphrase" placeholder="(Max 32, Alphanumeric)" autocomplete="off" value="{{fs_passphrase}}"/>
</div>
